
            <!-- On affiche si non vide -->
            <% @afficher = false %>

            <!-- s il y a des profils correspondant -->
            <% if @profils %>
                <% @afficher = true %>
                <%= render template: "profils/index" %>
            <% end %>

            <!-- s il y a des events correspondant -->
            <% if @events %>
                <% @afficher = true %>
                <%= render template: "events/index" %>
            <% end %>

            <% if !@afficher %>
                <% flash[:notice] = "Aucun résultat à votre recherche" %>
                <%= render (:back) %>
            <% end %>

